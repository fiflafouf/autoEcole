<%= simple_form_for(@user) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :email %>
    <%= f.input :role %>
    <%= f.input :nom %>
    <%= f.input :prenom %>
    <%= f.input :surnom %>
    <%= f.input :addr %>
    <%= f.input :cp %>
    <%= f.input :ville %>
    <%= f.input :dateNaiss %>
    <%= f.input :telFixe %>
    <%= f.input :telMobile %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
