<%= simple_form_for(@eleve_examan) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :examenId %>
    <%= f.input :eleveId %>
    <%= f.input :passer %>
    <%= f.input :score %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
