<p id="notice"><%= notice %></p>

<h1>Listing Assister Lecon Codes</h1>

<table>
  <thead>
    <tr>
      <th>Lecon code</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assister_lecon_codes.each do |assister_lecon_code| %>
      <tr>
        <td><%= assister_lecon_code.lecon_code_id %></td>
        <td><%= assister_lecon_code.user_id %></td>
        <td><%= link_to 'Show', assister_lecon_code %></td>
        <td><%= link_to 'Edit', edit_assister_lecon_code_path(assister_lecon_code) %></td>
        <td><%= link_to 'Destroy', assister_lecon_code, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Assister lecon code', new_assister_lecon_code_path %>
